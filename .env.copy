# Prompture Environment Variables
# Copy this file to .env and fill in your API keys.

# ─── Default Provider ────────────────────────────────────────────────────────
# Set AI_PROVIDER to one of: ollama, openai, claude, google, groq, grok,
# openrouter, azure, lmstudio, moonshot, zai, modelscope, cachibot
AI_PROVIDER=ollama

# ─── OpenAI ──────────────────────────────────────────────────────────────────
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o-mini

# ─── Claude (Anthropic) ─────────────────────────────────────────────────────
CLAUDE_API_KEY=your-claude-api-key
CLAUDE_MODEL=claude-haiku-4-5-20251001

# ─── Ollama (local) ─────────────────────────────────────────────────────────
OLLAMA_ENDPOINT=http://localhost:11434
OLLAMA_MODEL=llama3.1:8b

# ─── LM Studio (local) ──────────────────────────────────────────────────────
LMSTUDIO_ENDPOINT=http://127.0.0.1:1234/v1/chat/completions
LMSTUDIO_MODEL=deepseek/deepseek-r1-0528-qwen3-8b
LMSTUDIO_API_KEY=

# ─── Azure OpenAI (default backend) ─────────────────────────────────────────
AZURE_API_KEY=
AZURE_API_ENDPOINT=
AZURE_DEPLOYMENT_ID=
AZURE_API_VERSION=2025-01-01-preview

# Azure Claude Backend (optional, for claude-* models on Azure)
AZURE_CLAUDE_API_KEY=
AZURE_CLAUDE_ENDPOINT=
AZURE_CLAUDE_API_VERSION=

# Azure Mistral Backend (optional, for mistral-*/mixtral-* models on Azure)
AZURE_MISTRAL_API_KEY=
AZURE_MISTRAL_ENDPOINT=
AZURE_MISTRAL_API_VERSION=

# ─── Google Gemini ───────────────────────────────────────────────────────────
GOOGLE_API_KEY=your-google-api-key
GOOGLE_MODEL=gemini-2.5-flash

# ─── Groq ────────────────────────────────────────────────────────────────────
GROQ_API_KEY=your-groq-api-key
GROQ_MODEL=llama-3.3-70b-versatile

# ─── OpenRouter ──────────────────────────────────────────────────────────────
OPENROUTER_API_KEY=your-openrouter-api-key
OPENROUTER_MODEL=openai/gpt-4o-mini

# ─── Grok (xAI) ─────────────────────────────────────────────────────────────
GROK_API_KEY=your-grok-api-key
GROK_MODEL=grok-4-fast-reasoning

# ─── Moonshot AI (Kimi) ─────────────────────────────────────────────────────
MOONSHOT_API_KEY=your-moonshot-api-key
MOONSHOT_MODEL=kimi-k2-0905-preview
MOONSHOT_ENDPOINT=https://api.moonshot.ai/v1

# ─── Z.ai (Zhipu AI) ────────────────────────────────────────────────────────
ZHIPU_API_KEY=your-zhipu-api-key
ZHIPU_MODEL=glm-4.7
ZHIPU_ENDPOINT=https://api.z.ai/api/paas/v4

# ─── ModelScope (Alibaba Cloud) ─────────────────────────────────────────────
MODELSCOPE_API_KEY=your-modelscope-api-key
MODELSCOPE_MODEL=Qwen/Qwen3-235B-A22B-Instruct-2507
MODELSCOPE_ENDPOINT=https://api-inference.modelscope.cn/v1

# ─── CachiBot ───────────────────────────────────────────────────────────────
CACHIBOT_API_KEY=

# ─── HuggingFace ─────────────────────────────────────────────────────────────
HF_ENDPOINT=
HF_TOKEN=

# ─── Local HTTP ──────────────────────────────────────────────────────────────
LOCAL_HTTP_ENDPOINT=http://localhost:8000/generate
LOCAL_HTTP_MODEL=local-model

# ─── AirLLM ─────────────────────────────────────────────────────────────────
AIRLLM_MODEL=meta-llama/Llama-2-7b-hf
AIRLLM_COMPRESSION=

# ─── Stability AI (image generation) ────────────────────────────────────────
STABILITY_API_KEY=